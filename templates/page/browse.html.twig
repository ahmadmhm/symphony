{% extends 'base.html.twig' %}
{% block title %} Here Browsing {{ parent()}} {% endblock %}
{% block body %}
<h1>
    {{ title }}
</h1>
<h3>
    {{ 'My motto' }}
</h3>
<h2>
    <ul>
        {% for page in pages %}
            <li>
                {{ page.title }} ## <strong>{{ page.item | upper|lower}}</strong>
                <a class="btn btn-primary" href="{{ path('app_browse_page', {slug: page.title}) }}">
                    ->
                </a>
            </li>
        {% endfor %}
    </ul>
</h2>
{% endblock %}

